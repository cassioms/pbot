{
  "name": "pbot",
  "version": "1.0.0",
  "description": "A discord bot",
  "main": "index.js",
  "scripts": {
    "debug": "nodemon src/index.ts",
    "convert": "ts-jest config:migrate ./jest.config.js",
    "build": "tsc",
    "lint:list": "eslint */**/*.js --fix",
    "lint": "eslint */**/*.js --fix",
    "start:nodemon": "nodemon src/index.ts",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:prod": "node dist/index.js"
  },
  "author": "ppoli",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.1.0",
    "discord.js": "^12.1.1",
    "dotenv": "^8.2.0",
    "text-to-mp3": "^1.0.4",
    "text-to-speech-js": "^1.1.9",
    "text2wav": "^0.0.13",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333",
    "ytdl-core-discord": "^1.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.3.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9"
  }
}
